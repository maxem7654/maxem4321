import turtle                         
t = turtle.Turtle() 
s = turtle.Screen()
t.pencolor("green")
s.bgcolor("black") # change the background color
t.speed(0) #change the speed

for i in range(150): # the speed of drawing and background color
    # Draw shapes
    t.circle(190-i,90)
    t.lt(90)
    t.circle(190-i,90)  
    t.lt(18) 
    
    # Complete the drawing done()
    
